---
layout: layout
---

<div role="list" class="content-grid">

  {%- for item in items -%}
    <div role="listitem">
      <a href="{{ item.url }}" class="tool-wrap" target="_blank">
        <h3>{{ item.title }}</h3>
        <p class="quote">
          {{ item.quote }}
        </p>
      </a>
    </div>
  {% endfor %}

</div>
